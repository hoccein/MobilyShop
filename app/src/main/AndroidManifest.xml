<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ynwa.kdl.hosein.shopping">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mvp.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mvp.login.LoginActivity" />
        <activity android:name=".mvp.signup.SignUpActivity" />
        <activity android:name=".InsertDataActivity" />
        <activity android:name=".mvp.profile.ProfileActivity" />
        <activity android:name=".activity_without_mvp.PhoneDetailActivity" />
        <activity android:name=".activity_without_mvp.MorePhonesActivity" />
        <activity android:name=".activity_without_mvp.SearchActivity"></activity>
    </application>

</manifest>